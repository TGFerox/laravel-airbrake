{
    "name": "tgferox/laravel-airbrake",
    "type": "library",
    "description": "Laravel service provider for Airbrake https://github.com/airbrake/phpbrake",
    "keywords": ["airbrake", "laravel"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wmde/phpbrake.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "wmde/phpbrake": "dev-update-monolog",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Theo Kouzelis",
            "email": "tkouzelis@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kouz\\LaravelAirbrake\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kouz\\LaravelAirbrake\\ServiceProvider"
            ]
        }
    }
}
